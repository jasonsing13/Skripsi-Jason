<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Goods Received</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <%- include('partials/style.ejs') %>
</head>
<body>
    <%- include('partials/sidebar.ejs') %>
    <div class="container mt-4">
        <h1>Goods Received Vendor</h1>
        <form action="/goods-received-vendor" method="post" enctype="multipart/form-data">
            <div class="mb-3">
                <label for="invoice" class="form-label">Upload Invoice</label>
                <input type="file" class="form-control" id="invoice" name="invoice">
            </div>
            <div class="mb-3">
                <label for="surat_jalan" class="form-label">Upload Surat Jalan</label>
                <input type="file" class="form-control" id="surat_jalan" name="surat_jalan">
            </div>
            <button type="submit" class="btn btn-primary">Submit Hasil</button>
        </form>
    </div>
<%- include('partials/footer.ejs') %>
</html>